---
import Section from '../Section.astro'

import { languages } from '@cv'
---

<Section title='Language'>
	{
		languages.map(({ language, fluency }) => {
			return (
				<h3>
					{language} - <p>{fluency}</p>
				</h3>
			)
		})
	}
</Section>

<style>
	h3 {
		display: flex;
		font-weight: 500;
		color: #111;
		gap: 4px;
	}
</style>
